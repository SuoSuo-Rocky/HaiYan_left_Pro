<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--此文件为 数据库 表的 映射 文件 -->
<!-- 下方写 映射文件的路径 namespace : 该 mapper.xml映射文件的唯一标识符 -->
<mapper namespace="org.wang.mapper.MonkeyCardMapper" >
    
    <!--查询 猴子证信息  -->
    <select id="queryCardById" parameterType="int" resultType="monkeyCard">
        select * from monkeyCard where cardid=#{cardId}
    </select>
   
    <!--此SQL 语句的  id: org.wang.mapper.MonkeyCardMapper.queryCardById -->
    


</mapper>