package org.wang.entity;

public class Animal {
	
	private int id;
	private String name;
	private int kind;
	
	public Animal() {
		
	}

	
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getKind() {
		return kind;
	}

	public void setKind(int kind) {
		this.kind = kind;
	}
	
	
	@Override
	public String toString() {
		return "ID=" + this.id + "NAME=" + this.name + "KIND=" + this.kind;
	}
	
	

}
